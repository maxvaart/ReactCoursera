r((leader)=>leader.featured)[0]} \r\n        ></Home>\r\n      )\r\n    }\r\n\r\n  return (\r\n    <div>\r\n      <Header></Header>\r\n      <Switch>\r\n        <Route path=\"/home\" component={HomePage}></Route>\r\n        <Route exact path=\"/menu\" component={()=><Menu dishes={this.props.dishes}></Menu>}></Route>\r\n        <Route exact path=\"/contactus\" component={() => <Contact resetFeedbackForm={this.props.resetFeedbackForm} />}></Route>\r\n        <Route path='/menu/:dishId' component={DishWithId}></Route>\r\n        <Route exact path='/aboutus' component={()=><About leaders={this.props.leaders}></About>}></Route>\r\n        <Redirect to=\"/home\"></Redirect>\r\n      </Switch>\r\n      <Footer></Footer>\r\n    </div>\r\n  );\r\n}\r\n}\r\n\r\nexport default withRouter(connect(mappropsToProps, mapDispatchProps)(Main));\r\n","D:\\Maxi\\Cursos\\COURSERA\\REACT\\Confusion\\src\\redux\\configureStore.js",[],"D:\\Maxi\\Cursos\\COURSERA\\REACT\\Confusion\\src\\components\\HomeComponent.js",["227"],"import react from 'react';\r\nimport {Card, CardImg,CardText,CardBody,CardTitle,CardSubtitle} from 'reactstrap';\r\nimport {Loading} from './LoadingComponent';\r\nimport {baseUrl} from '../shared/baseUrl';\r\n\r\nfunction RenderCard({item, isLoading, errMess}){\r\n    if (isLoading){\r\n        return(\r\n            <Loading />\r\n        )\r\n    }\r\n    else if (errMess){\r\n        return(\r\n            <h4>{errMess}</h4>\r\n        )\r\n    } else {\r\n    return(\r\n        <Card>\r\n            <CardImg src={baseUrl + item.image} alt={item.name}></CardImg>\r\n            <CardBody>\r\n                <CardTitle>{item.name}</CardTitle>\r\n                {item.designation ? <CardSubtitle>{item.designation}</CardSubtitle> : null}\r\n                <CardText>{item.description}</CardText>\r\n            </CardBody>\r\n        </Card>\r\n    );\r\n    }\r\n}\r\n\r\nfunction Home(props){\r\n    return(\r\n        <div className=\"container\">\r\n            <div cla